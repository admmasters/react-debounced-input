{
  "name": "react-debounced-input",
  "version": "0.4.0",
  "description": "A React input component supporting debounce",
  "main": "./dist/ReactDebouncedInput.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info --content-base examples/",
    "test": "cross-env tsc && ava",
    "test:watch": "ava --watch --verbose && tsc -w",
    "clean:build": "rm -rf dist/* && npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/admmasters/react-debounced-input.git"
  },
  "keywords": [
    "input",
    "debounce",
    "react",
    "component"
  ],
  "author": "Matt Revell",
  "license": "MIT",
  "devDependencies": {
    "ava": "^2.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "jquery": "^3.4.1",
    "jsdom": "^15.1.1",
    "path": "^0.12.7",
    "raw-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2",
    "typings": "^2.1.1",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.2",
    "webpack-dev-server": "^3.7.2"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "ava": {
    "files": [
      "js/**/**/*.test.js"
    ],
    "source": [
      "js/**/*.test.{js,jsx}"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "typings": "./index.d.ts"
}
