{
  "name": "react-debounced-input",
  "version": "0.2.0",
  "description": "A React input component supporting debounce",
  "main": "./dist/ReactDebouncedInput-v0.2.0.js",
  "scripts": {
    "start": "cross-env webpack-dev-server --config ./webpack/webpack.examples.js",
    "test": "cross-env mocha --timeout 15000 --compilers js:babel-register --require ignore-styles ./**/*.spec.js -w",
    "test:watch": "cross-env mocha --timeout 15000 --compilers js:babel-register --require ignore-styles ./**/*.spec.js -w",
    "clean:build": "rm -rf dist/* && npm run build:dev && npm run build:prod",
    "build:dev": "cross-env webpack --config ./webpack/webpack.dev.js",
    "build:prod": "cross-env webpack --config ./webpack/webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/admmasters/react-debounced-input.git"
  },
  "keywords": [
    "input",
    "debounce",
    "react",
    "component"
  ],
  "author": "Matt Revell",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.0",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "enzyme": "^2.2.0",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-react": "^5.0.1",
    "ignore-styles": "^1.2.0",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "react-addons-test-utils": ">=0.14.0",
    "tslint": "^3.8.1",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  }
}
