{
  "name": "react-highlighted-text",
  "version": "0.0.7",
  "description": "React components to highlight text",
  "main": "./dist/ReactHighlightedText-v0.0.7.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.examples.js",
    "test": "mocha --timeout 15000 --compilers js:babel-register --require ignore-styles ./**/*.spec.js",
    "test:watch": "npm test -- -w",
    "clean:build": "rm -rf dist/* && npm run build:dev && npm run build:prod",
    "build:dev": "./node_modules/.bin/webpack --config ./webpack/webpack.dev.js",
    "build:prod": "./node_modules/.bin/webpack --config ./webpack/webpack.prod.js",
    "publish:new": "npm run clean:build && npm publish",
    "republish": "npm run clean:build && npm unpublish react-standard-table --force && npm publish"
  },
  "keywords": [
    "React",
    "table",
    "highlighted",
    "text"
  ],
  "author": "Qinec Ltd",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-react": "^4.2.3",
    "ignore-styles": "^1.2.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.8",
    "react-standard-table": "^0.0.6",
    "recompose": "^0.15.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react-highlighter": "^0.3.1"
  },
  "peerDependencies": {
    "react": "^15.0.0-0 || ^0.14.0",
    "react-dom": "^15.0.0-0 || 0.14.0"
  }
}
