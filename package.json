{
  "name": "react-highlighted-text",
  "version": "0.2.0-rc",
  "description": "React components to highlight text",
  "main": "./dist/ReactHighlightedText.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.examples.js",
    "test2": "mocha --timeout 15000 --compilers js:babel-register --require ignore-styles ./**/*.spec.js",
    "test": "karma start karma.conf.js",
    "test:watch": "npm test -- -w",
    "clean:build": "rm -rf dist/* && npm run build:prod",
    "build:dev": "webpack",
    "build:prod": "webpack --config webpack.prod.js",
    "publish:new": "npm run clean:build && npm publish",
    "republish": "npm run clean:build && npm unpublish react-highlighted-text --force && npm publish"
  },
  "keywords": [
    "React",
    "table",
    "highlighted",
    "text"
  ],
  "author": "Matt Revell",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-react": "^4.2.3",
    "ignore-styles": "^1.2.0",
    "jasmine-core": "^2.4.1",
    "jasmine-expect-jsx": "^1.1.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-jasmine": "^0.3.8",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-test-utils": "^0.14.8",
    "react-select": "^1.0.0-beta12",
    "react-standard-table": "^0.0.6",
    "recompose": "^0.15.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.7.4",
    "typescript": "^1.8.10",
    "webpack": "1.13.0",
    "webpack-config": "^4.0.0",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^15.0.0-0 || ^0.14.0",
    "react-dom": "^15.0.0-0 || 0.14.0"
  },
  "dependencies": {
    "react-highlighter": "^0.3.1"
  }
}
